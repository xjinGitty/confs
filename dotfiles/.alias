alias ..='cd ..'
alias ...='cd ../..'

alias l='ls -alF'
alias la='ls -la'

alias httpserver='python3 -m http.server '
alias kvm-openqa='qemu-kvm -m 1024 -net user -net nic,model=virtio -soundhw ac97 -smp 1 -enable-kvm'
alias kvm-bridge='qemu-kvm -m 1024 -net nic,model=virtio,macaddr=52:54:32:56:43:12 -net bridge,br=br0 -soundhw ac97 -enable-kvm'
alias qemu-snap='qemu-img snapshot'
alias j='jobs -l'
alias vi=vim
alias mount-view='mount | column -t'
alias xprop-wm='xprop _NET_WM_PID'

alias pd='pushd '
alias pd-='popd'
alias pd--='popd -2'
alias pd---='popd -3'
alias d='dirs -v'
alias b='pushd +1'
alias f='pushd -0'

alias cp='rsync -avhP'

alias openqa-web='/usr/bin/perl -w  /usr/share/openqa/script/openqa daemon -m development --proxy'
alias openqa-work='/usr/bin/perl -w /usr/share/openqa/script/worker  --instance 1 --verbose'
